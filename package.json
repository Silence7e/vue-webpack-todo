    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.1.6",
    "css-loader": "^0.28.11",
    "eslint": "^5.2.0",
    "eslint-config-airbnb-base": "^13.0.0",
