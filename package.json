{
  "name": "vue-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.conf.client.js",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.conf.server.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "practice": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.conf.practice.js",
    "clean": "rimraf public && rimraf server-build",
    "lint": "eslint --ext .js --ext .jsx --ext .vue client/",
    "lint-fix": "eslint --fix --ext .js --ext .jsx --ext .vue client/",
    "precommit": "lint-staged",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.conf.client.js",
    "dev:server": "nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "start": "cross-env NODE_ENV=production node server/server.js"
  },
  "author": "",
  "license": "ISC",
